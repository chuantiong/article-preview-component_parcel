n    background-color: $light-grayish-blue;\n    display: grid;\n    place-items: center;\n    height: 100vh;\n}\n\nh1,\np {\n    margin: 0;\n}\n\nimg {\n    width: 100%;\n}","src/scss/_variables.scss":"// colors\n$white-clr: #fff;\n$very-dark-grayish-blue: #48556A;\n$desaturated-dark-blue: #6E8098;\n$grayish-blue: #9DAEC2;\n$light-grayish-blue: #ECF2F8;\n\n// font family\n$primary-ff: 'Manrope', sans-serif;\n\n// font weight\n$fw-medium: 500;\n$fw-bold: 700;\n\n// font size\n$root-fs: 16px;\n$fs-13px: $root-fs / 1.23;\n$fs-16px: $root-fs;\n$fs-20px: $root-fs * 1.25;","src/scss/_article-component.scss":".article-component {\n    display: flex;\n    flex-direction: column;\n    margin: 0 auto;\n    width: 100%;\n    min-width: 327px;\n    max-width: 730px;\n    border-radius: 0.625rem;\n    background: $white-clr;\n    box-shadow: 0px 40px 40px -10px rgba(201, 213, 225, 0.50);\n\n    &__image-wrapper {\n        width: 100%;\n        display: flex;\n        border-top-left-radius: 0.625rem;\n        border-top-right-radius: 0.625rem;\n        overflow: hidden;\n        \n        .image {\n            min-width: clamp(327px, 10vw, 285px);\n            height: clamp(200px, 43vw, 280px);\n            object-fit: cover;\n        }\n    }\n\n    &__contents-container {\n        padding: 2.25rem 2rem 1.25rem;\n    }\n\n    &__title {\n        color: $very-dark-grayish-blue;\n        font-size: $fs-16px;\n        font-weight: $fw-bold;\n        line-height: 1.5rem;\n        letter-spacing: 0.0125rem;\n        margin-bottom: .75em;\n    }\n\n    &__description {\n        color: $desaturated-dark-blue;\n        font-size: $fs-13px;\n        font-weight: $fw-medium;\n        line-height: 1.25rem;\n        letter-spacing: 0.00763rem;\n    }\n\n    &__infos-container {\n        display: flex;\n        align-items: center;\n        gap: 1em;\n\n        &.infos--light {\n            padding: .75em 2em 1.25em;\n        }\n\n        &.desktop--light {\n            display: none;\n        }\n    \n        &.mobile--dark {\n            padding: 1em 2em;\n        }\n\n        &.desktop--dark {\n            width: fit-content;\n            padding: 1.19em 2.25em;\n            border-radius: 0.625rem;\n            box-shadow: 0px 10px 10px 0px rgba(201, 213, 225, 0.50);\n            position: absolute;\n            top: -200%;\n            left: 0;\n            transform: translateX(-44%);\n\n            &::before {\n                content: \"\";\n                background-color: $very-dark-grayish-blue;\n                width: 16px;\n                height: 16px;\n                position: absolute;\n                rotate: 45deg;\n                bottom: -22%;\n                left: 50%;\n                transform: translateX(-50%);\n            }\n        }\n\n    }\n\n    &__infos-primary-container {\n        position: relative;\n        margin-left: auto;\n    }\n\n    &__infos-container.infos--dark {\n        background-color: $very-dark-grayish-blue;\n\n        .social-icons-wrapper {\n            display: flex;\n            align-items: center;\n            gap: 1em;\n        }\n\n        .social-media-icon {\n            width: 20px;\n            user-select: none;\n        }\n    }\n\n    // Share mobile light\n\n    &__avatar {\n        width: 40px;\n        height: 40px;\n        border-radius: 50%;\n    }\n\n    &__name,\n    &__date {\n        font-size: $fs-13px;\n        line-height: 1.25rem;\n        letter-spacing: 0.00763rem;\n    }\n\n    &__name {\n        color: $very-dark-grayish-blue;\n    }\n\n    &__date {\n        color: $grayish-blue;\n    }\n\n    &__share-icon-wrapper {\n        width: 37px;\n        height: 37px;\n        display: grid;\n        place-items: center;\n        margin-left: auto;\n        border-radius: 50%;\n        cursor: pointer;\n        transition: transform 500ms ease-in-out, opacity 300ms ease-in-out;\n\n        &:hover {\n            opacity: .7;\n            transform: scale(1.12);\n        }\n\n        &.share-icon-wrapper--light {\n            background-color: $light-grayish-blue;\n        }\n\n        &.share-icon-wrapper--dark {\n            background-color: $desaturated-dark-blue;\n        }\n    }\n\n    &__share-icon {\n        width: 15px;\n        height: 13px;\n        pointer-events: none;\n        user-select: none;\n    }\n\n    &__share-text {\n        color: $grayish-blue;\n        font-size: $fs-13px;\n        text-transform: uppercase;\n        line-height: 1.25rem;\n        letter-spacing: 0.3125rem;\n        user-select: none;\n    }\n}","src/scss/_attribution.scss":"/* Footer */\n\n.attribution {\n    font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;\n    background: #F2F2F2;\n    opacity: .9;\n    padding: 2em;\n    position: fixed;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    z-index: 100;\n\n    &__text {\n        font-size: 11px;\n        text-align: center;\n        margin: 0;\n    }\n\n    & a {\n        color: hsl(228, 45%, 44%);\n    }\n}","src/scss/_utilities.scss":"// Utilities \n\n.hide {\n    display: none;\n}","src/scss/_media-queries.scss":"@media (min-width: 600px) {\n    .article-component {\n        flex-direction: row;\n\n        &__contents-container {\n            padding: 2rem 2.5rem 1.25rem;\n        }\n\n        &__title {\n            font-size: $fs-20px;\n            line-height: 1.75rem;\n            letter-spacing: 0.01563rem;\n            margin-bottom: .6em;\n        }\n\n        &__image-wrapper {\n            border-top-right-radius: 0;\n            border-bottom-left-radius: 0.625rem;\n\n            .image {\n                height: auto;\n                object-position: left;\n            }\n        }\n\n        &__infos-container.share-mobile--light {\n            padding: 0 2.5rem 2rem;\n        }\n\n        // Hide \n\n        &__infos-container {\n            &.mobile--light {\n                display: none;\n            }\n\n            &.desktop--light {\n                display: flex;\n            }\n\n            &.mobile--dark {\n                display: none;\n            }\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"1746bf740d1752b5388678dd82d26273","cacheData":{"env":{}}}